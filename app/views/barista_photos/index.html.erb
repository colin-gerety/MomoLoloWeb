<p id="notice"><%= notice %></p>

<h1>Barista Photos</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Title</th>
      <th>Note</th>
      <th>Can display</th>
      <th>Primary photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @barista_photos.each do |barista_photo| %>
      <tr>
        <td><%= barista_photo.image %></td>
        <td><%= barista_photo.title %></td>
        <td><%= barista_photo.note %></td>
        <td><%= barista_photo.can_display %></td>
        <td><%= barista_photo.primary_photo %></td>
        <td><%= link_to 'Show', barista_barista_photo_path(barista_photo.barista.id, barista_photo.id) %></td>
        <td><%= link_to 'Edit', edit_barista_barista_photo_path(barista_photo.barista.id, barista_photo.id) %></td>
        <td><%= link_to 'Destroy', barista_barista_photo_path(barista_photo.barista.id, barista_photo.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Barista Photo', new_barista_barista_photo_path %>
