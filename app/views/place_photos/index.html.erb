<p id="notice"><%= notice %></p>

<h1>Place Photos</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Title</th>
      <th>Note</th>
      <th>Can display</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @place_photos.each do |place_photo| %>
      <tr>
        <td><%= place_photo.image %></td>
        <td><%= place_photo.title %></td>
        <td><%= place_photo.note %></td>
        <td><%= place_photo.can_display %></td>
        <td><%= link_to 'Show', place_photo %></td>
        <td><%= link_to 'Edit', edit_place_photo_path(place_photo) %></td>
        <td><%= link_to 'Destroy', place_photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place Photo', new_place_photo_path %>
