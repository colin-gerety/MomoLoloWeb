</head>

<body>
  <div id="appBlocker"></div>
  <%= render :partial => 'header' %>
<main>
  <p id="notice"><%= notice %></p>
  <section>
    <%= yield %>
  </section>
</main>
  <div id="footerSpacer"></div>
  <%= render :partial => 'footer' %>
</body>
</html>
