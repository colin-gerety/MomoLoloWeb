<p id="notice"><%= notice %></p>

<h1>Baristas</h1>

<table>
  <thead>
    <tr>
      <th>First</th>
      <th>Last</th>
      <th>Currently working</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @baristas.each do |barista| %>
      <tr>
        <td><%= barista.first %></td>
        <td><%= barista.last %></td>
        <td><%= barista.currently_working %></td>
        <td><%= link_to 'Show', barista %></td>
        <td><%= link_to 'Edit', edit_barista_path(barista) %></td>
        <td><%= link_to 'Destroy', barista, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Barista', new_barista_path %>
